{
  "root": "libs/somelib",
  "sourceRoot": "libs/somelib/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/somelib",
        "tsConfig": "libs/somelib/tsconfig.lib.json",
        "packageJson": "libs/somelib/package.json",
        "main": "libs/somelib/src/index.ts",
        "assets": ["libs/somelib/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/somelib/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/somelib"],
      "options": {
        "jestConfig": "libs/somelib/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
